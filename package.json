{
	"name": "regexper-unofficial",
	"displayName": "Regexper unofficial",
	"description": "Regexper for vscode",
	"publisher": "aperricone",
	"author": "aperricone",
	"version": "0.0.2",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/APerricone/vscode-regexper-unofficial"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"regex", "regexper", "visualizer"
	],
	"activationEvents": [
		"onCommand:regexper.showRegEx"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [ {
				"command": "regexper.showRegEx",
				"title": "%regexper.cmd%"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Regexper unofficial",
			"properties": {
				"regexper-unofficial.useOnePanel": {
					"type": "boolean",
					"description": "%regexper.useOnePanel%",
					"default": true
				},
				"regexper-unofficial.translate": {
					"type": "boolean",
					"description": "%regexper.translate%",
					"default": true
				}
			}
		},
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"vscode-nls": "^4.1.1"
	}
}
